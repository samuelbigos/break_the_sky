; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Break The Sky"
run/main_scene="res://scenes/metagame/Game.tscn"
config/features=PackedStringArray("4.2", "C#")
config/icon="res://icon.png"
config/save_version=1

[audio]

buses/default_bus_layout="res://assets/sfx/buses/default_bus_layout.tres"

[autoload]

DebugImGui="*res://src/debug/DebugImGui.cs"
SceneTransitionManager="*res://src/singletons/SceneTransitionManager.cs"
MusicPlayer="*res://src/singletons/MusicPlayer.cs"
ColourManager="*res://scenes/singletons/ColourManager.tscn"
Resources="*res://scenes/singletons/Resources.tscn"
Metagame="*res://scenes/metagame/Metagame.tscn"
FlockingManager="*res://src/steering/SteeringManager.cs"
DatabaseWaves="*res://scenes/database/DatabaseWaves.tscn"
FabricateManager="*res://scenes/singletons/FabricateManager.tscn"
SaveDataSettings="*res://src/singletons/SaveDataSettings.cs"
SaveDataPlayer="*res://src/singletons/SaveDataPlayer.cs"
SaveDataWorld="*res://src/singletons/SaveDataWorld.cs"
SaveManager="*res://src/singletons/SaveManager.cs"
TimeSystem="*res://src/singletons/TimeSystem.cs"
BoidFactory="*res://src/boids/BoidFactory.cs"
ParticleManager="*res://scenes/singletons/ParticleManager.tscn"
ImGuiRoot="*res://addons/imgui-godot/data/ImGuiRoot.tscn"

[display]

window/size/viewport_width=1440
window/size/viewport_height=810
window/vsync/vsync_mode=false

[dotnet]

project/assembly_name="Break The Sky"

[editor_plugins]

enabled=PackedStringArray("res://addons/imgui-godot/plugin.cfg")

[gd2cs]

config/ask_no_python=true
config/python="py"

